import @io;
module HelloWorld is Entry{

    func sum(...args:int[]) -> int{
        let sum:int = 0;
        for(let i:int = 0;i<len(args);i=i+1){
            sum = sum + args[i];
        }
        return sum;
    }

    func main(argc:int, argv:string*) -> int{
        return sum(1, 2, 3);
    } 



}


